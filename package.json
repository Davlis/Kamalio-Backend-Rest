{
  "name": "kamalio",
  "version": "0.0.1",
  "main": "server.js",
  "scripts": {
    "test": "npm run test",
    "build": "babel src -d dist",
    "start": "node --harmony dist/server",
    "watch": "babel -w src -d dist",
    "watch-d": "babel -w src -d dist & nodemon --harmony dist/server",
    "drop-db": "node ./dist/tools/drop-database.js",
    "sync-db": "node ./dist/tools/sync-database.js",
    "seed-db": "node ./dist/tools/seed-database.js",
    "postinstall": "babel src -d dist",
    "heroku-postbuild": "node ./dist/tools/sync-database.js"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "body-parser": "^1.18.3",
    "boom": "^5.2.0",
    "cloudinary": "^1.11.0",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "geolib": "^2.0.24",
    "jsonwebtoken": "^8.1.0",
    "morgan": "^1.9.0",
    "multiparty": "^4.1.4",
    "pg": "^6.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.13.15"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "nodemon": "^1.12.1"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0,
      "semi": 0,
      "indent": [
        "error",
        4
      ]
    }
  },
  "engines": {
    "node": ">=10.2.0"
  }
}
